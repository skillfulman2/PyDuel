  <script>

// Javascript function to get cookie and read its value
function getCookie(cname) {
  var name = cname + "=";
  var ca = document.cookie.split(';');
  for(var i = 0; i < ca.length; i++) {
    var c = ca[i];
    while (c.charAt(0) == ' ') {
      c = c.substring(1);
    }
    if (c.indexOf(name) == 0) {
      return c.substring(name.length, c.length);
    }
  }
  return "";
}


// enter the name of the cookie you use to determine if the user is a subscriber here
  var cookieValueNew = getCookie("subscriber");
  var publir_show_ads = true;

  if(cookieValueNew != "") {
      publir_show_ads = false;
    }

  if (publir_show_ads) {
        var googletag = googletag || {};
            googletag.cmd = googletag.cmd || [];

            (function() {
                var opxsTK1 = document.createElement("script");
                opxsTK1.type = "text/javascript";
                opxsTK1.async = true;
                opxsTK1.src = "//a.publir.com/platform/434.js";
                var opxsTargetTK1 = document.getElementsByTagName("head")[0];
                opxsTargetTK1.insertBefore(opxsTK1, opxsTargetTK1.firstChild);
